live_loop :piano do
  play :f4
  sleep 1
  play :f4
  sleep 2
  play :fs4
  sleep 1
  play :f4
  sleep 1
  play :as3
  sleep 0.5
  play :as3
  sleep 1
  play :as3
  sleep 0.5
  play :fs4
  sleep 1
end

sleep 16

